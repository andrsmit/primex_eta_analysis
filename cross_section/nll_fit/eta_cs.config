7#################################################################################
# DATA SELECTION:

primexPhase: 3

angularRange: 0.0 4.5

#
#Different options for the cut variations:
#  0. All default cuts applied
#  1. 3-d invariant mass matrix
#  2. FCAL cuts varied
#  5. TOF cuts varied
#  8. Photon angles are shifted/smeared for Coherent MC. For data, this is the same as if analysisOption=0
#
analysisOption: 0
#
# Veto option for default analysis (0-8)
#
vetoOption: 6

#
# Name of histogram to access from data root files:
# (note: this is only important if analysisOption is NOT 0 or 1)
#
#histName:    mgg_singleTOF

7#################################################################################
# MC SELECTION (for lineshape of mgg spectrum and for angular matrix):
#   These are by default set equal to the above values (for data). But will be 
#   overwritten if they are uncommented.
#

#analysisOption_MC: 8
#vetoOption_MC:     6

#histName_MC: mgg_const_zShift_00
#matrixName:  dirZ/AngularMatrix_zShift_00

#--------------------------------------------#
# Model Used for Fitting Yield Distribution:

ModelType: SGEVORKYAN_AP_VAR
ModelParameter_ap: 4

#ModelType: SGEVORKYAN_STRONG_RADIUS_VAR
#ModelParameter_strongRadiusVer: 0

#ModelType: SGEVORKYAN_AP_VAR
#ModelParameter_ap: 4

#ModelType: SGEVORKYAN_SIGMA_VAR
#ModelParameter_sigma: 4

#--------------------------------------------#
# other options:

#FCAL fiducial cut
#histName:   FidCut/mgg_FCAL_fid_12
#matrixName: FidCut/AngularMatrix_FCALFidCut_12

#FCAL energy cut
#histName:   ECut/mgg_FCAL_ecut_19
#matrixName: ECut/AngularMatrix_FCALECut_19

#FCAL extra energy cut:
#histName:   ExtraECut/mgg_FCAL_extra_ecut_04
#matrixName: ExtraECut/AngularMatrix_FCALExtraECut_04

#TOF strict veto:
#histName:   mgg_singleTOF
#matrixName: AngularMatrix_singleTOF

#TOF Timing cuts (ranges from 0.5ns to 2.25ns in 0.25ns increments, 0-7):
#histName:   TOFTimingCut/mgg_TOFTiming_00
#matrixName: TOFTimingCut/AngularMatrix_TOFTimingCut_00

#Smearing photon angles:
#matrixName: AngularMatrix_AngularSmear_06

#Shifting photon angles:
#matrixName: AngularMatrix_AngularShift_03

#Varying placement of elasticity cut on left side of peak:
# (00: 10sigma, 01: 7.5sigma, 02: 5sigma, 03: 4sigma, 04: 3sigma, 05: 2sigma, 06: 1sigma)
#histName:   mgg_Elasticity_00
#matrixName: AngularMatrix_Elasticity_00

#--------------------------------------------#

#
# Beam energy range:
# (note: This is only editable whenever analycp sisOption=1.
#   Otherwise, the beam energy range is fixed to 9.0-10.9 GeV)
#
beamEnergy: 8.0 11.3
#
# Binning:
#
rebinsMgg:   4
rebinsTheta: 5

#################################################################################
# FITTING OPTIONS:

#
#Different options for fitting the eta->2gamma signal:
#  1. Use histograms from signal MC + histogram of eta+pion background mc + histogram of additional backgrounds
#  2. Use parameterized lineshapes from signal MC + histogram of eta+pion background mc + histogram of additional backgrounds
#
signalFitOption: 3

#
# Parameter to decide where to get lineshape for fitting invariant mass
#   0. Coherent Grid MC
#   1. BGGEN
#   2. Hybrid (will use coherent for angles < 1.5deg and bggen above)
# (Only used for signalFitOptions 5+)
#
lineshapeOption: 0

#
#Different options for fitting the eta->2gamma background:
#	0. Background fixed to zero (no additional background)
#	1. polynomial
#	2. exponential function
#	3. Chebyshev Polynomial
#	4. Analytical approximation of e+e- background shape: dsigma/dM ~ (1/M)*(ln(2*E/M) - 0.5)
#
bkgdFitOption:   2
polynomialOrder: 2

#
#Different options for fitting the omega->pi0+gamma background:
#   0. No fit performed. However, if the specified fitting range exceeds mgg>0.725, this is changed to option 1.
#	1. Crystal Ball function
#	2. Fit simulated histogram to data
#	3. Extract parameterized lineshape from simulation and fit to data
#   4. Extract parameterized lineshape from simulation and fit to data - allowing width of lineshape to float
#
omegaFitOption:  3

#
#Different options for fitting the rho0 background:
#   0. No fit performed.
#	1. Extract a guess for this lineshape by fitting omega->pi0+gamma histogram
#	2. Extract a guess for this lineshape by fitting omega->pi0+gamma histogram, then constrain relative fraction of 0.05 < rho/omega < 0.15
#	3. Extract parameterized lineshape from simulation and fit to data (not currently implemented)
#   4. Extract parameterized lineshape from simulation and fit to data - with relative normalization to omega fixed (not currently implemented) 
#
rhoFitOption:  0

#
#Different options for fitting the omega->pi0+gamma background:
#	0. Don't try to fit eta' peak
#	1. Do try to fit it
#
etapFitOption:   0


fittingRange: 0.300 0.925

#################################################################################
#
# [0: no fit to eta, 1: Gaussian, 2: lineshape]
#
emptyFitOption_eta: 2

#
# [0: no fit to omega, 1: Crystal Ball, 2: lineshape]
#
emptyFitOption_omega: 1

#
# [0: no fit to fdc peaks, 1: Gaussians, 2: lineshape with common normalization, 3: lineshape with separate normalizations]
#
emptyFitOption_fdc: 1

#
# [1: polynomial, 2: exponential, 3: chebyshev]
#
emptyFitOption_bkgd: 2
emptyFitOption_poly: 3

emptyFittingRange: 0.300 0.925

rebinsEmptyMgg: 4

#################################################################################
